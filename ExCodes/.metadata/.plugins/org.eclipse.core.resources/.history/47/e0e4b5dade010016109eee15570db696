

public class Exp {
	double base;
	int exp;
	
	double getValue() {
		double res = 1;
		for(int i=0; i<exp; i++) res *= base;
		return res;
	}
	
	public static void main(String[] arg) {
		Exp exp1 = new Exp();
		exp1.base = 1.0;
		exp1.exp = 300000000;
		
		System.out.println("value = " + exp1.getValue());
	}
}
